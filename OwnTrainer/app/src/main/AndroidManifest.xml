<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.brolly.owntrainer" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".TurnOn"
            android:label="@string/app_name"
            android:screenOrientation="portrait" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MyMenu"
            android:label="@string/title_activity_menu"
            android:screenOrientation="portrait" >
            <intent-filter>
                <action android:name=".MyMenu" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Tabata"
            android:label="@string/title_activity_tabata"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name=".MuscleMass"
            android:label="@string/title_activity_muscle_mass"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:screenOrientation="portrait"
            android:name=".Spartakus"
            android:label="@string/title_activity_spartakus" >
        </activity>
    </application>

</manifest>
